{
  "manifest_version": 3,
  "name": "More Tab Colors",
  "version": "1.1",
  "description": "Change the color of your tabs",
  "permissions": [
    "theme",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Change tab color"
  }
}
